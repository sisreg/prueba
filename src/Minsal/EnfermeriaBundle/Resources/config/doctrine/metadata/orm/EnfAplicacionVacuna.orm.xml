<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EnfAplicacionVacuna" table="enf_aplicacion_vacuna">
    <indexes>
      <index name="fki_catalogo_producto_aplicacion_vacuna" columns="id_catalogo_producto"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="edadMinima" type="smallint" column="edad_minima" nullable="true"/>
    <field name="unidadMinima" type="string" column="unidad_minima" length="10" nullable="true"/>
    <field name="edadMaxima" type="smallint" column="edad_maxima" nullable="true"/>
    <field name="unidadMaxima" type="string" column="unidad_maxima" length="10" nullable="true"/>
    <field name="viaAdministracion" type="string" column="via_administracion" length="20" nullable="true"/>
    <many-to-one field="idCatalogoProducto" target-entity="FarmCatalogoProducto">
      <join-columns>
        <join-column name="id_catalogo_producto" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
