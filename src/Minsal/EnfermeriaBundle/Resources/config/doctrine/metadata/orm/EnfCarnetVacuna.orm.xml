<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EnfCarnetVacuna" table="enf_carnet_vacuna">
    <indexes>
      <index name="IDX_7AA0120701624C4" columns="id_expediente"/>
      <index name="IDX_7AA01207DFA12F6" columns="id_establecimiento"/>
      <index name="IDX_7AA0120890253C7" columns="id_empleado"/>
      <index name="IDX_7AA0120BE654634" columns="id_aplicacion_dosis"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <field name="fecha" type="date" column="fecha" nullable="true"/>
    <many-to-one field="idAplicacionDosis" target-entity="EnfAplicacionDosis">
      <join-columns>
        <join-column name="id_aplicacion_dosis" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEmpleado" target-entity="MntEmpleado">
      <join-columns>
        <join-column name="id_empleado" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idEstablecimiento" target-entity="CtlEstablecimiento">
      <join-columns>
        <join-column name="id_establecimiento" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
    <many-to-one field="idExpediente" target-entity="MntExpediente">
      <join-columns>
        <join-column name="id_expediente" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
