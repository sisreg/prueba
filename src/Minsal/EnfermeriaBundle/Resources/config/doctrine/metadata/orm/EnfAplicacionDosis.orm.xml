<?xml version="1.0" encoding="utf-8"?>
<doctrine-mapping xmlns="http://doctrine-project.org/schemas/orm/doctrine-mapping" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://doctrine-project.org/schemas/orm/doctrine-mapping http://doctrine-project.org/schemas/orm/doctrine-mapping.xsd">
  <entity name="EnfAplicacionDosis" table="enf_aplicacion_dosis">
    <indexes>
      <index name="fki_aplicacion_vacuna_dosis" columns="id_aplicacion_vacuna"/>
    </indexes>
    <id name="id" type="integer" column="id">
      <generator strategy="SEQUENCE"/>
    </id>
    <many-to-one field="idAplicacionVacuna" target-entity="EnfAplicacionVacuna">
      <join-columns>
        <join-column name="id_aplicacion_vacuna" referenced-column-name="id"/>
      </join-columns>
    </many-to-one>
  </entity>
</doctrine-mapping>
