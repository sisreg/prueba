	<queryString>
		<![CDATA[select primer_nombre,segundo_nombre,tercer_nombre,primer_apellido,segundo_apellido,
apellido_casada,fecha_nacimiento,hora_nacimiento,
K.nombre as pais_nacimiento,
C1.nombre as municipio_nacimiento,
L.nombre as estado_civil,I.nombre as id_doc_paciente,A.numero_doc_ide_paciente,
M.nombre as ocupacion,direccion,telefono_casa,
B.nombre as departamento_domicilio,C.nombre as municipio_domicilio,
H.nombre as canton,G.nombre area_geografica,lugar_trabajo,telefono_trabajo,
J.nombre asarea_cotizacion, asegurado,numero_afiliacion,A.cotizante,A.nombre_padre,
A.nombre_madre,A.nombre_conyuge,
nombre_responsable,direccion_responsable,telefono_responsable,D.parentesco,
I2.nombre as nombre_responsable,numero_doc_ide_responsable,
nombre_proporciono_datos,I1.nombre,numero_doc_ide_propor_datos,I1.nombre,
observacion,conocido_por,F.nacionalidad,
E.nombre as sexo,B1.nombre as departamento_nacimiento
from mnt_paciente A
inner join ctl_departamento B on (A.id_departamento_domicilio=B.id)
inner join ctl_municipio C on (A.id_municipio_domicilio=C.id)
inner join ctl_departamento B1 on (A.id_departamento_nacimiento=B1.id)
inner join ctl_municipio C1 on (A.id_municipio_nacimiento=C1.id)
inner join ctl_parentesco D on (A.id_parentesco_responsable=D.id)
inner join ctl_parentesco D1 on (A.id_parentesco_propor_datos=D1.id)
inner join ctl_sexo E on (A.id_sexo=E.id)
inner join ctl_area_geografica G on (A.area_geografica_domicilio=G.id)
left join ctl_nacionalidad F on (A.id_nacionalidad=F.id)
left join ctl_canton H on (A.id_canton_domicilio=H.id)
inner join ctl_documento_identidad I on (a.id_doc_ide_paciente=I.id)
inner join ctl_documento_identidad I1 on (a.id_doc_ide_proporciono_datos=I1.id)
inner join ctl_documento_identidad I2 on (a.id_doc_ide_responsable=I1.id)
left join ctl_area_cotizante J on (A.id_area_cotizacion=J.id)
inner join ctl_pais K on (A.id_pais_nacimiento=K.id)
inner join ctl_estado_civil L on (A.id_estado_civil=L.id)
inner join ctl_ocupacion M on (A.id_ocupacion=M.id)


where A.id=5468]]>
	</queryString>
